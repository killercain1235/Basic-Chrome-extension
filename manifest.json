{
    "manifest_version":2,
    "name":"Basic-Chrome-extension",
    "version":"0.1",
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "code_50px.png"
    },
    "icons": {
        "50": "code_50px.png"
    }
}